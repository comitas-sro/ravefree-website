{
  "name": "ravefree-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "code-quality": "concurrently -m 1 'npm run lint' 'npm run build'",
    "dev": "next dev --webpack",
    "build": "next build --webpack && ./inline-css.mjs && next-export-optimize-images",
    "start": "next start",
    "lint": "concurrently -m 1 'npm run lint:eslint' 'npm run lint:typescript'",
    "lint:eslint": "eslint --concurrency auto",
    "lint:typescript": "next typegen && tsc",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.6",
    "@types/mdx": "^2.0.13",
    "next": "^16.1.6",
    "next-export-optimize-images": "^4.7.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.19.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "beasties": "^0.3.5",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.7.4",
    "typescript": "^5",
    "wrangler": "^4.60.0"
  },
  "overrides": {
    "next-export-optimize-images": {
      "next": "^16"
    }
  }
}
